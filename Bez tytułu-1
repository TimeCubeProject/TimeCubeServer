DEF INT _ERROR
DEF STRING[100] _N_WKS_DIR, _N_PLIK_CSV
DEF STRING[255] _ODCZYT[1]

_N_WKS_DIR = "PLIKI"

_N_PLIK_CSV = "TEST"

DELETE(_ERROR, _N_PLIK_CSV)

IF _ERROR <> 0

MSG("BLAD PODCZAS USUWANIA NR: " << _ERROR)
;G4 F100 - PRZERWA CZASOWA
ENDIF


WRITE(_ERROR, _N_PLIK_CSV, "HELLO,WORLD")

READ(_ERROR, _N_PLIK_CSV, 1, 1, _ODCZYT)

WRITE(_ERROR, _N_PLIK_CSV, _ODCZYT[0] << ",ODCZYTANY,TEKST")

M30